#include <stdio.h>
#include <stdlib.h>
#include <time.h>
/**
* main - Entry point of the program
*
* Return: Always 0 (success)
*/
int main(void)
{
int n, lastDigit;
srand(time(0));
n = rand() - RAND_MAX / 2;
lastDigit = n % 10;
printf("Last digit of n is int * and is ", n, lastDigit);
if (lastDigit > 5)
{
printf("Last digit of %n is %n and is greater than 5\n");
}
else if (lastDigit == 0)
{
printf("Last digit of %n is %n and is 0\n");
}
else
{
printf("Last digit of %n is %n and is less than 6 and not 0\n");
}

return (0);
}
